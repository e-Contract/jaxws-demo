<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxws="http://cxf.apache.org/jaxws"
       xmlns:cxf="http://cxf.apache.org/core"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
    http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd
    http://cxf.apache.org/core http://cxf.apache.org/schemas/core.xsd">
    
    <jaxws:endpoint id="be.e_contract.demo.ExampleServicePortImpl"
                     implementor="be.e_contract.demo.ExampleServicePortImpl"
                     address="http://localhost:8080/jaxws-demo/example">
        <jaxws:invoker>
            <bean class='org.jboss.wsf.stack.cxf.JBossWSInvoker'/>
        </jaxws:invoker>
    </jaxws:endpoint>
    
    <bean id="ECDSAAlgorithmSuiteLoader"
          class="be.e_contract.demo.ECDSAAlgorithmSuiteLoader" />
    <bean id="ECDSAInInterceptor"
          class="be.e_contract.demo.ECDSAInInterceptor" />
    <cxf:bus>
        <cxf:inInterceptors>
            <ref bean="ECDSAInInterceptor" />
        </cxf:inInterceptors>
    </cxf:bus>
    
</beans>
